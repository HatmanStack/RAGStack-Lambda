{
  "name": "@ragstack/ragstack-chat",
  "version": "1.0.0",
  "description": "Reusable React component for AI chat with source attribution using AWS Bedrock Knowledge Base",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./wc": {
      "types": "./dist/wc.d.ts",
      "import": "./dist/wc.esm.js",
      "require": "./dist/wc.js",
      "default": "./dist/wc.js"
    }
  },
  "scripts": {
    "build": "tsc && npm run build:wc",
    "build:wc": "node scripts/inject-amplify-config.js && vite build --config vite.wc.config.ts && node scripts/inject-css-to-bundle.js",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "check": "npm run lint && npm run test"
  },
  "keywords": [
    "ragstack",
    "bedrock",
    "chat",
    "ai",
    "react",
    "component",
    "rag",
    "source-attribution",
    "knowledge-base"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.50.0",
    "happy-dom": "^12.9.0",
    "knip": "^5.76.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^0.34.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/RAGStack-Lambda.git",
    "directory": "src/ragstack-chat"
  },
  "bugs": {
    "url": "https://github.com/your-org/RAGStack-Lambda/issues"
  },
  "homepage": "https://github.com/your-org/RAGStack-Lambda/tree/main/src/ragstack-chat#readme"
}
