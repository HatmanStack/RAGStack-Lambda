{
  "name": "ragstack-lambda",
  "version": "1.0.0",
  "description": "Serverless OCR-to-Knowledge Base pipeline on AWS",
  "private": true,
  "scripts": {
    "deploy": "./publish.sh",
    "lint:backend": "uv run ruff check . --fix && uv run ruff format .",
    "lint:frontend": "cd src/ui && npm run lint -- --fix",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "format": "uv run ruff format .",
    "format:check": "uv run ruff format . --check",
    "test:backend": "uv run pytest tests/unit/python/ -m 'not integration'",
    "test:backend:integration": "uv run pytest -m integration",
    "test:backend:coverage": "uv run pytest tests/unit/python/ -m 'not integration' --cov=lib --cov=src/lambda --cov-report=html --cov-report=term --cov-report=xml",
    "test:frontend": "cd src/ui && npm test -- --run",
    "test:amplify-chat": "cd src/amplify-chat && npm test -- --run",
    "test": "npm run test:backend && npm run test:frontend",
    "test:amplify": "cd amplify && npm test -- --run",
    "test:coverage": "npm run test:backend:coverage && cd src/ui && npm run test:coverage",
    "test:all": "npm run lint && npm run test && npm run test:amplify-chat && npm run test:amplify"
  },
  "keywords": [
    "aws",
    "lambda",
    "bedrock",
    "ocr",
    "rag",
    "serverless"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aws-amplify/backend": "^1.17.0",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@playwright/test": "^1.56.1",
    "@vitest/ui": "^4.0.6",
    "aws-cdk-lib": "^2.216.0",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.12",
    "happy-dom": "^20.0.10",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "aws-amplify": "^6.15.7"
  }
}
